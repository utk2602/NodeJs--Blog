<div class="author">
    <h1 class="author__heading">Hi, I am Utkarsh.</h1>
    <p class="author__body">Web developer and ECE student </p>
</div>

<img src="/img/hero-image.webp" alt="person looking out through window" class="hero-image" width="981" height="528">

<section class="articles">
    <h2 class="article__heading">Latest Posts</h2>

    <ul class="article-ul">

        <% data.forEach(Post => { %>
         
        
       <li>
        <a href="a">
            <span><% post.title %></span>
            <span class="article-list__date"><% post.createdAt %>/span>
        </a>
       </li>

       <% }) %>


    </ul>

    <a href="a" class="pagination">&lt;View Older Posts</a>
</section>